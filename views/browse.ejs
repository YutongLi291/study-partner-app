<%- include('partials/header'); -%>
<span style="font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;">
<h2>CPSC 213</h2>
<h1>Find Your Study Partner</h1>
<h3>View All Students</h3>
<button style="float:right; background-color:orange; font-family:'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;">My Profile</button>

</span>
<input type="text" value="Search Courses">
<ul>
    <% people.forEach(function(person) { %>
        <li>
            <strong><%= person.name %></strong>
            is taking <%= person.courses.sort().join(', ')  %>
        </li>
    <% }); %>
</ul>
<style>
h1 {
    text-align: left;
    font-family: 'Courier New';
    font-size: 25px;
    text-transform: uppercase;
}
h2 {
    text-align: left;
    font-family: 'Courier New';
    text-transform: uppercase;
    font-size: 20px;
}
h3 {
    text-align: left;
    font-family: 'Courier New';
    font-size: 40px;
    font-weight: bold;
    text-transform: uppercase;
    padding-top: 20px;
    padding-bottom: 20px;
}
</style>
<%- include('partials/footer'); -%>