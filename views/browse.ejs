<%- include('partials/header'); -%>
<span style="font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;">
<h1>
  Find Your Study Partner   <button style="float:right; background-color:orange; font-family:'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;">My Profile</button>
</h1>
</span>
<ul>
    <% people.forEach(function(person) { %>
        <li>
            <strong><%= person.name %></strong>
            is taking <%= person.courses.sort().join(', ')  %>
        </li>
    <% }); %>
</ul>

<%- include('partials/footer'); -%>